---
interface Props {
  color?: string;
  size?: string;
  id: string;
  style: string;
}

const { color = "inherit", size = "24", id, style } = Astro.props;
---

<svg
  id={id}
  class={style}
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 -960 960 960"
  height={`${size}px`}
  width={`${size}px`}
  fill={color}
  ><path
    d="M120-240v-80h720v80H120Zm0-200v-80h720v80H120Zm0-200v-80h720v80H120Z"
  ></path></svg
>
